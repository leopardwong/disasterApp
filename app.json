{
    "expo": {
        "name": "DisasterApp",
        "slug": "DisasterApp",
        "scheme": "disasterapp",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./src/assets/icon.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./src/assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            }
        },
        "web": {
            "favicon": "./src/assets/favicon.png"
        },
        "plugin": [
            [
                "expo-notifications",
                {
                    "icon": "./src/assets/caution.png",
                    "color": "#ffffff",
                    "androidMode": "default"
                }
            ]
        ]
    }
}
